{
  "name": "@ahur-system/ai-rate-limiter",
  "description": "Intelligent API Rate Limiting with AI-powered adaptive throttling",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "rate-limiting",
    "api",
    "ai",
    "redis",
    "throttling",
    "adaptive",
    "strategy-pattern",
    "php",
    "composer"
  ],
  "homepage": "https://github.com/yourusername/ai-rate-limiter",
  "support": {
    "issues": "https://github.com/yourusername/ai-rate-limiter/issues",
    "source": "https://github.com/yourusername/ai-rate-limiter"
  },
  "authors": [
    {
      "name": "Ahur System",
      "email": "developer@ahursystem.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "ext-redis": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "AhurSystem\\AIRateLimiter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AhurSystem\\AIRateLimiter\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "phpstan": "phpstan analyse src tests",
    "install": "composer install",
    "build": "composer install --no-dev --optimize-autoloader"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-rate-limiter.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-rate-limiter/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/yourusername/ai-rate-limiter"
  },
  "engines": {
    "php": ">=8.1"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  }
} 