{
    "name": "ahur-system/ai-rate-limiter",
    "description": "Intelligent API Rate Limiting with AI-powered adaptive throttling",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "rate-limiting",
        "api",
        "ai",
        "redis",
        "throttling",
        "adaptive",
        "strategy-pattern"
    ],
    "homepage": "https://github.com/ahur-system/ai-rate-limiter",
    "support": {
        "issues": "https://github.com/ahur-system/ai-rate-limiter/issues",
        "source": "https://github.com/ahur-system/ai-rate-limiter"
    },
    "authors": [
        {
            "name": "Ahur System",
            "email": "apps.khaleghi@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-redis": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "AhurSystem\\AIRateLimiter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhurSystem\\AIRateLimiter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "phpstan": "phpstan analyse src tests"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 